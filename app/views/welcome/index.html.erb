<h1>  Hello word~~~</h1>

<div class="container">	
	<div class="row">
		<% @product.each do |product| %>
			<div class="col-xs-6 col-md-3">
				<P><%= image_tag(product.file_location, :width => 50) %></P>
				<p><%= link_to product.name %></p>
				<p>$<%= product.price %></p>
			</div>
			<% end %>
	</div>
</div>

